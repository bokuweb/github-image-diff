{
  "manifest_version": 2,
  "name": "Github image diff",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "webNavigation",
    "https://render.githubusercontent.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": ["content.js", "dom.js"],
      "all_frames": true
    }
  ]
}
